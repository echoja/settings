      "label": "go",
      "kind": "command",
      "target": "go",
      "install": "https://go.dev/doc/install"
    },
    {
      "label": "go-bin",
      "kind": "dir",
      "target": "/usr/local/go/bin",
      "depends": [
        "go"
      ]
    },
    {
      "label": "vscode-code",
      "kind": "command",
      "target": "code",
      "depends": [
        "vscode-app-bin"
      ]
    },
    {
      "label": "kubectl",
      "kind": "command",
