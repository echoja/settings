{
  "$schema": "./deps.schema.json",
  "checks": [
    {
      "label": "homebrew",
      "kind": "command",
      "target": "brew",
      "install": "https://brew.sh/"
    },
    {
      "label": "git",
      "kind": "command",
      "target": "git"
    },
    {
      "label": "oh-my-zsh",
      "kind": "file",
      "target": "$HOME/.oh-my-zsh/oh-my-zsh.sh",
      "depends": [
        "git"
      ],
      "install": "https://ohmyz.sh/#install"
    },
    {
      "label": "powerlevel10k",
      "kind": "dir",
      "target": "$HOME/.oh-my-zsh/custom/themes/powerlevel10k",
      "depends": [
        "oh-my-zsh"
      ],
      "install": "https://github.com/romkatv/powerlevel10k"
    },
    {
      "label": "zsh-syntax-highlighting",
      "kind": "dir",
      "target": "$HOME/.oh-my-zsh/custom/plugins/zsh-syntax-highlighting",
      "depends": [
        "oh-my-zsh"
      ],
      "install": "https://github.com/zsh-users/zsh-syntax-highlighting"
    },
    {
      "label": "fzf",
      "kind": "command",
      "target": "fzf",
      "depends": [
        "homebrew"
      ]
    },
    {
      "label": "direnv",
