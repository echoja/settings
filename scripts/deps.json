{
  "$schema": "./deps.schema.json",
  "checks": [
    { "label": "homebrew", "pattern": "/opt/homebrew/bin/brew", "kind": "command", "target": "brew" },
    { "label": "git", "pattern": "plugins=\\(git", "kind": "command", "target": "git" },
    { "label": "oh-my-zsh", "pattern": "oh-my-zsh.sh", "kind": "file", "target": "$HOME/.oh-my-zsh/oh-my-zsh.sh" },
    { "label": "powerlevel10k", "pattern": "powerlevel10k", "kind": "dir", "target": "$HOME/.oh-my-zsh/custom/themes/powerlevel10k" },
    { "label": "zsh-syntax-highlighting", "pattern": "zsh-syntax-highlighting", "kind": "dir", "target": "$HOME/.oh-my-zsh/custom/plugins/zsh-syntax-highlighting" },
    { "label": "fzf", "pattern": "fzf --zsh", "kind": "command", "target": "fzf" },
    { "label": "direnv", "pattern": "direnv hook zsh", "kind": "command", "target": "direnv" },
    { "label": "zoxide", "pattern": "zoxide init zsh", "kind": "command", "target": "zoxide" },
    { "label": "go", "pattern": "go env GOPATH", "kind": "command", "target": "go" },
    { "label": "go-bin", "pattern": "/usr/local/go/bin", "kind": "dir", "target": "/usr/local/go/bin" },
    { "label": "vscode-code", "pattern": "code --locate-shell-integration-path", "kind": "command", "target": "code" },
    { "label": "kubectl", "pattern": "kubectl", "kind": "command", "target": "kubectl" },
    { "label": "kubectx", "pattern": "kubectx", "kind": "command", "target": "kubectx" },
    { "label": "pnpm", "pattern": "pnpm", "kind": "command", "target": "pnpm" },
    { "label": "curlie", "pattern": "curlie", "kind": "command", "target": "curlie" },
    { "label": "codex", "pattern": "codex", "kind": "command", "target": "codex" },
    { "label": "terraform", "pattern": "terraform", "kind": "command", "target": "terraform" },
    { "label": "nvim", "pattern": "nvim", "kind": "command", "target": "nvim" },
    { "label": "tv", "pattern": "tv text", "kind": "command", "target": "tv" },
    { "label": "rg", "pattern": "rg --files", "kind": "command", "target": "rg" },
    { "label": "python3", "pattern": "python3", "kind": "command", "target": "python3" },
    { "label": "pip3", "pattern": "pip3", "kind": "command", "target": "pip3" },
    { "label": "npm", "pattern": "npm config", "kind": "command", "target": "npm" },
    { "label": "mysql-client", "pattern": "/opt/homebrew/opt/mysql-client/bin", "kind": "dir", "target": "/opt/homebrew/opt/mysql-client/bin" },
    { "label": "vscode-app-bin", "pattern": "Visual Studio Code.app/Contents/Resources/app/bin", "kind": "dir", "target": "/Applications/Visual Studio Code.app/Contents/Resources/app/bin" },
    { "label": "choose", "pattern": "choose 0", "kind": "command", "target": "choose" },
    { "label": "raycast", "kind": "dir", "target": "/Applications/Raycast.app" },
    { "label": "tailscale", "kind": "dir", "target": "/Applications/Tailscale.app" }
  ]
}
